{
  "name": "recipeasy",
  "version": "1.0.0",
  "description": "Recipeasy UI",
  "main": "index.js",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "npm run clean && webpack --config tools/webpack/dev.js --progress --profile --bail",
    "build:prod": "npm run clean && webpack --config tools/webpack/prod.js --progress --profile --bail",
    "clean": "rimraf dist dll coverage",
    "clean:dist": "rimraf dist",
    "commit-msg": "validate-commit-msg",
    "lint": "eslint ./src",
    "karma": "karma start --single-run",
    "karma:watch": "karma start --watch",
    "setup": "npm install && npm start",
    "server": "webpack-dev-server --config tools/webpack/dev.js --progress --profile --watch --content-base src/",
    "start": "npm run server",
    "test": "npm run karma"
  },
  "repository": "github.com/hamishhossack/recipeasy",
  "keywords": [
    "Reapeasy",
    "ES6",
    "Webpack"
  ],
  "author": "Hamish Hossack",
  "license": "ISC",
  "dependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
    "animate.css": "^3.5.2",
    "autoprefixer": "^7.1.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "commitizen": "^2.9.6",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "handlebars": "^4.0.8",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.2",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.3",
    "optimize-js-plugin": "^0.0.4",
    "postcss-loader": "^2.0.5",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.0",
    "sass-loader": "^6.0.5",
    "siema": "^1.4.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.17.0",
    "to-string-loader": "^1.1.5",
    "validate-commit-msg": "^2.12.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0",
    "xhr-promise": "^2.0.0"
  }
}
